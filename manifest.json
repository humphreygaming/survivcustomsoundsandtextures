{
    "manifest_version": 2,
    "name": "survivcustomsounds",
    "author":"humphreygaming, brzsmg",
    "version": "1.0.0",
    "description": "Mod for surviv.io",
    "icons": {
        "128": "ext_128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "ext_128.png"
        }
    },
    "background": {
        "scripts": [ "background.js" ]
    },
    "web_accessible_resources":[
        "audio/*",
        "img/*"
    ],
    "permissions": [ "storage", "webRequest", "webRequestBlocking", "contextMenus", "https://surviv.io/" ]
}